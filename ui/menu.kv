# menu.kv

<Menu>:
    id: tela_administrador
    BoxLayout:
        orientation: 'horizontal'

        # Menu lateral
        BoxLayout:
            orientation: 'vertical'
            size_hint_x: 0.25
            padding: 20
            spacing: 20
            canvas.before:
                Color:
                    rgba: 0.2, 0.2, 0.2, 1
                Rectangle:
                    pos: self.pos
                    size: self.size

            # Imagem no topo do menu
            Image:
                source: 'data/logo.png'  # Caminho atualizado para a imagem
                size_hint_y: None
                height: 100
                allow_stretch: True
                keep_ratio: True
                pos_hint: {'center_x': 0.5}
            Label:
                text: 'Usuario:'
                size_hint_y: None
                height: 20
            BoxLayout:
                orientation: 'vertical'
                size_hint_x: 0.25
                size_hint_y: 0.15
                pos_hint: {'center_x': 0.5}
                padding: 20
                spacing: 0  # Remove o espaçamento entre os widgets

                Button:
                    id: identificador_id
                    text: 'ID'
                    size_hint_y: None
                    height: 20  # Altura do primeiro botão

                Button:
                    id: identificador_cpf
                    text: 'CPF'
                    size_hint_y: None
                    height: 20  # Altura do segundo botão
            Label:
                text: 'Menu'
                size_hint_y: None
                height: 40
                color: 1, 1, 1, 1
                font_size: 24
                halign: 'center'

            Button:
                text: 'Convenios'
                size_hint_y: None
                height: 40
                on_release: root.ids.screen_manager.current = 'convenios'
            Button:
                text: 'Especialidades'
                size_hint_y: None
                height: 40
                on_release: root.ids.screen_manager.current = 'especialidades'
            Button:
                text: 'Funcionarios'
                size_hint_y: None
                height: 40
                on_release: root.ids.screen_manager.current = 'funcionarios'
            Button:
                text: 'Medicos'
                size_hint_y: None
                height: 40
                on_release: root.ids.screen_manager.current = 'medicos'
            Button:
                text: 'Usuarios'
                size_hint_y: None
                height: 40
                on_release: root.ids.screen_manager.current = 'usuarios'

            Widget:  # Espaço flexível
                size_hint_y: 1

            Button:
                text: 'Sair'
                size_hint_y: None
                height: 40
                on_release: root.sair()

        # Área de conteúdo
        ScreenManager:
            id: screen_manager
            Screen:
                name: 'inicio'
                BoxLayout:
                    orientation: 'vertical'
                    Image:
                        source: 'data/logo.png'  # Caminho atualizado para a imagem
                        allow_stretch: True
                        keep_ratio: True

# Tela de Cadastro de Convenios ------------------------------------------------------------------

            Screen:
                name: 'convenios'
                BoxLayout:
                    orientation: 'horizontal'

                    BoxLayout:
                        orientation: 'vertical'

                        ScreenManager:
                            id: screen_manager_convenio

                            # Tela de Cadastro de Convenio
                            Screen:
                                name: 'cadastrar_convenio'
                                BoxLayout:
                                    orientation: 'vertical'
                                    padding: 20
                                    spacing: 20
                                    canvas.before:
                                        Color:
                                            rgba: 0.9, 0.9, 0.9, 1  # Fundo claro
                                        Rectangle:
                                            pos: self.pos
                                            size: self.size
                                    Label:
                                        text: 'Cadastro de Convenios'
                                        font_size: 44
                                        color: 0, 0, 0, 1
                                        size_hint_y: None
                                        height: 50
                                    ScrollView:
                                        BoxLayout:
                                            orientation: 'vertical'
                                            padding: [10, 50, 10, 10]
                                            spacing: 20
                                            size_hint_y: None
                                            height: self.minimum_height

                                            Label:
                                                text: 'Nome da Empresa:'
                                                font_size: 18
                                                color: 0, 0, 0, 1
                                                size_hint_x: None
                                                width: 155
                                                height: 40
                                            TextInput:
                                                id: nome_empresa_cadastrar
                                                hint_text: 'Nome da Empresa'
                                                multiline: False
                                                font_size: 18
                                                size_hint_y: None
                                                height: 40
                                            Label:
                                                text: 'CNPJ:'
                                                font_size: 18
                                                color: 0, 0, 0, 1
                                                size_hint_x: None
                                                width: 50
                                                height: 40
                                            TextInput:
                                                id: cnpj_cadastrar
                                                hint_text: 'CNPJ'
                                                multiline: False
                                                font_size: 18
                                                size_hint_y: None
                                                height: 40
                                            Label:
                                                text: 'Telefone:'
                                                font_size: 18
                                                color: 0, 0, 0, 1
                                                size_hint_x: None
                                                width: 75
                                                height: 40
                                            TextInput:
                                                id: telefone_convenio_cadastrar
                                                hint_text: 'Telefone'
                                                multiline: False
                                                font_size: 18
                                                size_hint_y: None
                                                height: 40
                                            Button:
                                                text: 'Cadastrar'
                                                size_hint_y: None
                                                height: 50
                                                on_release: root.cadastrar_convenio()

                            # Tela de listagem de Convenios
                            Screen:
                                name: 'listar_convenios'
                                on_enter: root.visualizar_convenio()
                                BoxLayout:
                                    orientation: 'vertical'
                                    padding: 20
                                    spacing: 20
                                    canvas.before:
                                        Color:
                                            rgba: 0.9, 0.9, 0.9, 1  # Fundo claro
                                        Rectangle:
                                            pos: self.pos
                                            size: self.size
                                    BoxLayout:
                                        size_hint_y: None
                                        height: 30
                                        canvas.before:
                                            Color:
                                                rgba: (0.2, 0.2, 0.2, 1)
                                            Rectangle:
                                                size: self.size
                                                pos: self.pos
                                        Label:
                                            text: 'Nome da Empresa'
                                        Label:
                                            text: 'CNPJ'
                                        Label:
                                            text: 'Telefone'

                                    GridLayout:
                                        id: convenios_grid
                                        cols: 1

                            # Tela de Editar Convenio
                            Screen:
                                name: 'editar_convenio'
                                BoxLayout:
                                    orientation: 'vertical'
                                    padding: 20
                                    spacing: 20
                                    canvas.before:
                                        Color:
                                            rgba: 0.9, 0.9, 0.9, 1  # Fundo claro
                                        Rectangle:
                                            pos: self.pos
                                            size: self.size
                                    Label:
                                        text: 'Editar Convenio'
                                        font_size: 44
                                        color: 0, 0, 0, 1
                                        size_hint_y: None
                                        height: 50

                                    BoxLayout:
                                        orientation: 'horizontal'
                                        padding: [10, 50, 10, 10]
                                        spacing: 20
                                        size_hint_y: None
                                        height: self.minimum_height

                                        TextInput:
                                            id: buscar_convenio_editar
                                            hint_text: 'CNPJ'
                                            multiline: False
                                            font_size: 18
                                            size_hint: None, None
                                            size: 200, 40

                                        Button:
                                            text: 'Buscar'
                                            size_hint: None, None
                                            size: 100, 40
                                            on_release: root.buscar_convenio()

                                    ScrollView:
                                        BoxLayout:
                                            orientation: 'vertical'
                                            padding: [10, 50, 10, 10]
                                            spacing: 20
                                            size_hint_y: None
                                            height: self.minimum_height
                                            id: dados_convenio
                                            Label:
                                                text: 'Nome da Empresa:'
                                                font_size: 18
                                                color: 0, 0, 0, 1
                                                size_hint_x: None
                                                width: 155
                                                height: 40
                                            TextInput:
                                                id: nome_empresa_editar
                                                hint_text: 'Nome da Empresa'
                                                multiline: False
                                                font_size: 18
                                                size_hint_y: None
                                                height: 40
                                            Label:
                                                text: 'CNPJ:'
                                                font_size: 18
                                                color: 0, 0, 0, 1
                                                size_hint_x: None
                                                width: 50
                                                height: 40
                                            TextInput:
                                                id: cnpj_editar
                                                hint_text: 'CNPJ'
                                                multiline: False
                                                font_size: 18
                                                size_hint_y: None
                                                height: 40
                                            Label:
                                                text: 'Telefone:'
                                                font_size: 18
                                                color: 0, 0, 0, 1
                                                size_hint_x: None
                                                width: 75
                                                height: 40
                                            TextInput:
                                                id: telefone_convenio_editar
                                                hint_text: 'Telefone'
                                                multiline: False
                                                font_size: 18
                                                size_hint_y: None
                                                height: 40
                                            Button:
                                                text: 'Salvar'
                                                size_hint_y: None
                                                height: 50
                                                on_release: root.editar_convenio()

                             # Tela de Excluir Convenio
                            Screen:
                                name: 'excluir_convenio'
                                BoxLayout:
                                    orientation: 'vertical'
                                    padding: 20
                                    spacing: 20
                                    canvas.before:
                                        Color:
                                            rgba: 0.9, 0.9, 0.9, 1  # Fundo claro
                                        Rectangle:
                                            pos: self.pos
                                            size: self.size
                                    Label:
                                        text: 'Excluir Convenio'
                                        font_size: 44
                                        color: 0, 0, 0, 1
                                        size_hint_y: None
                                        height: 50

                                    BoxLayout:
                                        orientation: 'horizontal'
                                        padding: [10, 50, 10, 10]
                                        spacing: 20
                                        size_hint_y: None
                                        height: self.minimum_height

                                        TextInput:
                                            id: buscar_convenio_excluir
                                            hint_text: 'CNPJ'
                                            multiline: False
                                            font_size: 18
                                            size_hint: None, None
                                            size: 200, 40

                                        Button:
                                            text: 'Buscar'
                                            size_hint: None, None
                                            size: 100, 40
                                            on_release: root.buscar_convenio()

                                    ScrollView:
                                        BoxLayout:
                                            orientation: 'vertical'
                                            padding: [10, 50, 10, 10]
                                            spacing: 20
                                            size_hint_y: None
                                            height: self.minimum_height
                                            id: dados_convenio
                                            Label:
                                                text: 'Nome da Empresa:'
                                                font_size: 18
                                                color: 0, 0, 0, 1
                                                size_hint_x: None
                                                width: 155
                                                height: 40
                                            TextInput:
                                                id: nome_empresa_excluir
                                                hint_text: 'Nome da Empresa'
                                                multiline: False
                                                readonly: True
                                                font_size: 18
                                                size_hint_y: None
                                                height: 40
                                            Label:
                                                text: 'CNPJ:'
                                                font_size: 18
                                                color: 0, 0, 0, 1
                                                size_hint_x: None
                                                width: 50
                                                height: 40
                                            TextInput:
                                                id: cnpj_excluir
                                                hint_text: 'CNPJ'
                                                multiline: False
                                                readonly: True
                                                font_size: 18
                                                size_hint_y: None
                                                height: 40
                                            Label:
                                                text: 'Telefone:'
                                                font_size: 18
                                                color: 0, 0, 0, 1
                                                size_hint_x: None
                                                width: 75
                                                height: 40
                                            TextInput:
                                                id: telefone_convenio_excluir
                                                hint_text: 'Telefone'
                                                multiline: False
                                                readonly: True
                                                font_size: 18
                                                size_hint_y: None
                                                height: 40
                                            Button:
                                                text: 'Excluir'
                                                size_hint_y: None
                                                height: 50
                                                on_release: root.excluir_convenio()

                        BoxLayout:
                            orientation: 'vertical'
                            size_hint_y: None
                            height: self.minimum_height
                            spacing: 10

                            canvas.before:
                                Color:
                                    rgba: 0.2, 0.2, 0.2, 1  # Cor para o fundo
                                Rectangle:
                                    size: self.size
                                    pos: self.pos

                            Label:
                                text: "Cadastrar/Listar/Editar/Excluir:"
                                font_size: 44
                                size_hint_y: None
                                height: 70

                            BoxLayout:
                                size_hint_y: None
                                height: 40
                                spacing: 10

                                Button:
                                    text: "Cadastrar Convenio"
                                    on_release: root.ids.screen_manager_convenio.current = 'cadastrar_convenio'

                                Button:
                                    text: "Listar Convenios"
                                    on_release: root.ids.screen_manager_convenio.current = 'listar_convenios'

                                Button:
                                    text: "Editar Convenio"
                                    on_release: root.ids.screen_manager_convenio.current = 'editar_convenio'

                                Button:
                                    text: "Excluir Convenio"
                                    on_release: root.ids.screen_manager_convenio.current = 'excluir_convenio'

                            Widget:  # Widget vazio para adicionar espaço abaixo dos botões
                                size_hint_y: None
                                height: 10  # Altura do espaço abaixo dos botões

