# Tela de Cadastro de Funcionarios ------------------------------------------------------------------

            Screen:
                name: 'funcionarios'
                BoxLayout:
                    orientation: 'horizontal'

                    BoxLayout:
                        orientation: 'vertical'

                        ScreenManager:
