# menuMedico.kv

<MenuMedico>:
    id: tela_medico
    BoxLayout:
        orientation: 'horizontal'

        # Menu lateral
        BoxLayout:
            orientation: 'vertical'
            size_hint_x: 0.25
            padding: 20
            spacing: 20
            canvas.before:
                Color:
                    rgba: 0.2, 0.2, 0.2, 1
                Rectangle:
                    pos: self.pos
                    size: self.size

            # Imagem no topo do menu
            Image:
                source: 'data/logo.png'  # Caminho atualizado para a imagem
                size_hint_y: None
                height: 100
                allow_stretch: True
                keep_ratio: True
                pos_hint: {'center_x': 0.5}
            Label:
                text: 'Usuario:'
                size_hint_y: None
                height: 20
            BoxLayout:
                orientation: 'vertical'
                size_hint_x: 0.25
                size_hint_y: 0.15
                pos_hint: {'center_x': 0.5}
                padding: 20
                spacing: 0  # Remove o espaçamento entre os widgets

                Button:
                    id: identificador_id
                    text: 'ID'
                    size_hint_y: None
                    height: 20  # Altura do primeiro botão

                Button:
                    id: identificador_cpf
                    text: 'CPF'
                    size_hint_y: None
                    height: 20  # Altura do segundo botão
            Label:
                text: 'Menu'
                size_hint_y: None
                height: 40
                color: 1, 1, 1, 1
                font_size: 24
                halign: 'center'

            Button:
                text: 'Consultas Agendadas'
                size_hint_y: None
                height: 40
                on_release: root.ids.screen_manager.current = 'consultas_agendadas'
            Button:
                text: 'Prontuarios'
                size_hint_y: None
                height: 40
                on_release: root.ids.screen_manager.current = 'prontuarios'
            Button:
                text: 'Exames'
                size_hint_y: None
                height: 40
                on_release: root.ids.screen_manager.current = 'exames'
            Button:
                text: 'Medicamentos'
                size_hint_y: None
                height: 40
                on_release: root.ids.screen_manager.current = 'medicamentos'

            Widget:  # Espaço flexível
                size_hint_y: 1

            Button:
                text: 'Sair'
                size_hint_y: None
                height: 40
                on_release: root.sair()

        # Área de conteúdo
        ScreenManager:
            id: screen_manager
            Screen:
                name: 'inicio'
                BoxLayout:
                    orientation: 'vertical'
                    Image:
                        source: 'data/logo.png'  # Caminho atualizado para a imagem
                        allow_stretch: True
                        keep_ratio: True

# Tela de Consultas Agendadas ---------------------------------------------------------

            Screen:
                name: 'consultas_agendadas'
                BoxLayout:
                    orientation: 'horizontal'

                    BoxLayout:
                        orientation: 'vertical'

                        ScreenManager:
                            id: screen_manager_agendamento

                            # Tela de listagem de Consultas
                            Screen:
                                name: 'listar_consultas'
                                on_enter: root.visualizar_consulta()
                                BoxLayout:
                                    orientation: 'vertical'
                                    padding: 20
                                    spacing: 20
                                    canvas.before:
                                        Color:
                                            rgba: 0.9, 0.9, 0.9, 1  # Fundo claro
                                        Rectangle:
                                            pos: self.pos
                                            size: self.size
                                    BoxLayout:
                                        size_hint_y: None
                                        height: 30
                                        canvas.before:
                                            Color:
                                                rgba: (0.2, 0.2, 0.2, 1)
                                            Rectangle:
                                                size: self.size
                                                pos: self.pos
                                        Label:
                                            text: 'Nome'
                                        Label:
                                            text: 'CPF'
                                        Label:
                                            text: 'Telefone Fixo'
                                        Label:
                                            text: 'Telefone Celular'
                                        Label:
                                            text: 'Data da Consulta'
                                        Label:
                                            text: 'Horario da Consulta'
                                        Label:
                                            text: 'Tem Convenio'
                                        Label:
                                            text: 'Nome Convenio'
                                        Label:
                                            text: 'Médico'
                                    GridLayout:
                                        id: consultas_grid
                                        cols: 1
                        BoxLayout:
                            orientation: 'vertical'
                            size_hint_y: None
                            height: self.minimum_height
                            spacing: 10

                            canvas.before:
                                Color:
                                    rgba: 0.2, 0.2, 0.2, 1  # Cor para o fundo
                                Rectangle:
                                    size: self.size
                                    pos: self.pos

                            Label:
                                text: "Listar:"
                                font_size: 44
                                size_hint_y: None
                                height: 70

                            BoxLayout:
                                orientation: 'vertical'
                                size_hint_y: None
                                height: 40
                                spacing: 10

                                Button:
                                    text: "Listar"
                                    size_hint_y: None
                                    size_hint_x: None
                                    height: 40
                                    width: 600
                                    pos_hint: {'center_x': 0.5}
                                    on_release: root.visualizar_consulta()

                            Widget:  # Widget vazio para adicionar espaço abaixo dos botões
                                size_hint_y: None
                                height: 10  # Altura do espaço abaixo dos botões

